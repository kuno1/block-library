{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"jquery": true
	},
	"globals": {
		"Vue": false,
		"kbl": true
	},
	"extends": [
		"plugin:@wordpress/eslint-plugin/recommended"
	],
	"rules": {
		"object-shorthand": "off",
		"prettier/prettier": "off",
		"jsx-a11y/click-events-have-key-events": "off",
		"jsx-a11y/no-noninteractive-element-interactions": "off"
	}
}

